<div class="page-header">
  <h3>Ranking #<%= @ranking.id %></h3>
</div>

<div class="row">
  <div class="col-md-12">
    <dl class="dl-horizontal">
      <dt>Link</dt>
      <dd><%= @ranking.link %></dd>

      <dt>Year</dt>
      <dd><%= @ranking.year %></dd>

      <dt>Rank</dt>
      <dd><%= @ranking.rank %></dd>

      <dt>Report name</dt>
      <dd><%= @ranking.report_name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/rankings" class="btn btn-primary">
        Back
      </a>
      <a href="/rankings/<%= @ranking.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_ranking/<%= @ranking.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
